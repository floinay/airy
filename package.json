{
  "name": "airy-ui",
  "version": "2.0.1",
  "scripts": {
    "serve": "ng serve",
    "watch-tp": "ng build typography --watch --configuration production",
    "build-tp": "ng build typography --configuration production",
    "publish-tp": "npm run build-tp && cd dist/typography && npm publish --access public",
    "build and publish tp": "npm run build-tp && npm run publish-tp",
    "watch cdk": "ng build cdk --watch --configuration production",
    "build-cdk": "ng build cdk --configuration production",
    "publish cdk": "npm run build-cdk && cd dist/cdk && npm publish",
    "link cdk": "cd dist/cdk && npm link",
    "watch layout": "ng build layout --watch --configuration production",
    "build-layout": "ng build layout --configuration production",
    "publish layout": "npm run build-layout && cd dist/layout && npm publish",
    "link layout": "cd dist/layout && npm link",
    "watch controls": "ng build controls --watch --configuration production",
    "build controls": "ng build controls --configuration production",
    "publish controls": "cd dist/controls && npm publish",
    "build-direction": "ng build direction --configuration production",
    "watch direction": "ng build direction --watch",
    "link direction": "cd dist/direction && npm link",
    "publish direction": "npm run build-direction && cd dist/direction && npm publish",
    "build-icon": "ng build icon --configuration production",
    "publish icon": "npm run build-icon && cd dist/icon && npm publish --access=public",
    "build-button": "ng build button --configuration production",
    "publish button": "npm run build-button && cd dist/button && npm publish --access=public"
  },
  "private": true,
  "dependencies": {
    "@airy-ui/layout": "^2.1.2",
    "@airy-ui/button": "^2.0.0",
    "@airy-ui/cdk": "2.1.2",
    "@airy-ui/direction": "^2.1.0",
    "@angular/animations": "~12.1.0",
    "@angular/cdk": "^12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "@ng-web-apis/common": "^1.9.0",
    "@ngneat/until-destroy": "^8.0.3",
    "lodash-es": "^4.17.21",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.1.0-next.0",
    "typescript": "~4.3.2"
  }
}
